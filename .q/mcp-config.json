        "ide-assistant"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "brave-search-mcp"
      ],
      "env": {
        "BRAVE_API_KEY": "BSAnvp-gWxQKFORwwgR_9MSr0VG3odR"
      },
      "timeout": 120000,
      "disabled": false
    },
    "supabase": {
      "command": "supabase-mcp-server",
