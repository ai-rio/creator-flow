{
  "$schema": "https://raw.githubusercontent.com/aws/amazon-q-developer-cli/refs/heads/main/schemas/agent-v1.json",
  "name": "creator-flow-main",
  "description": "Main CreatorFlow development agent with comprehensive project context and TikTok Shop expertise",
  "prompt": "You are a CreatorFlow development specialist focused on TikTok Shop fulfillment automation. Follow all project standards and business requirements.",
  "mcpServers": {
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "brave-search-mcp"
      ],
      "env": {
        "BRAVE_API_KEY": "BSAnvp-gWxQKFORwwgR_9MSr0VG3odR"
      },
      "timeout": 120000,
      "disabled": false
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "ide-assistant"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/carlos/projects"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "sqlite": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sqlite"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "fetch": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-fetch"
      ],
      "env": {},
      "timeout": 120000,
      "disabled": false
    },
    "context7": {
      "command": "npx",
      "args": [
        "@context7/mcp-server"
      ],
      "env": {
        "CTX7_API_KEY": "ctx7sk-8616b76b-ba9e-4db4-acc9-795ef3dc6c05"
      },
      "timeout": 120000,
      "disabled": false
    }
  },
  "tools": [
    "*"
  ],
  "toolAliases": {},
  "allowedTools": [
    "fs_read",
    "use_aws",
    "supabase___*",
    "memory___*",
    "brave_*",
    "filesystem___*",
    "github___*",
    "execute_bash",
    "browser_*",
    "fs_write"
  ],
  "resources": [
    "file://.q-rules.md",
    "file://README.md",
    "file://CLAUDE.md",
    "file://docs/**/*.md",
    "file://.claude/agents/**/*.md"
  ],
  "hooks": {},
  "toolsSettings": {},
  "useLegacyMcpJson": false
}